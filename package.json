{
  "name": "collab-code",
  "displayName": "CollabCode",
  "description": "A collaborative code editing extension",
  "version": "0.0.1",
  "publisher": "team-collab",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "collab-code.helloWorld",
